{
    "name": "AppItVentures/Forret",
    "description": "L4 Starter kit used produced by Forret",
    "authors": [
        {
            "name": "Michael Hopkins",
            "email": "michael@appitventures.com"
        }
    ],
	"require": {
		"laravel/framework": "4.2.*",
        "way/generators": "2.6.1",
        "iron-io/iron_mq": "1.5.1",
        "barryvdh/laravel-vendor-cleanup": "1.2.3",
        "fzaninotto/faker": "1.4.0",
        "barryvdh/laravel-ide-helper": "1.10.0",
        "barryvdh/laravel-debugbar": "1.6.4",
        "dingo/api": "0.6.2",
        "bugsnag/bugsnag-laravel": "1.0.8",
        "esensi/model": "0.3.1",
        "cartalyst/sentry": "2.1.3",
        "codeception/codeception": "2.0.1"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds"
		],
		"psr-0": {
            "Forret": "app/",
            "Controllers\\Api": "app/",
            "Controllers\\Admin": "app/",
            "Controllers\\Frontend": "app/"
		}
	},
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan debugbar:publish",
            "php artisan clear-compiled",
            "php artisan vendor-cleanup",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
